{
  "name": "@bridgecall/api",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "db:generate": "drizzle-kit generate --config ./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./drizzle.config.ts"
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bridgecall/shared": "workspace:*",
    "dotenv": "16.4.5",
    "fastify": "4.28.1",
    "pg": "8.12.0"
  },
  "devDependencies": {
    "@types/node": "20.14.11",
    "@types/pg": "8.11.6",
    "tsx": "4.15.6",
    "typescript": "5.5.3"
  }
}
