{
  "name": "@bridgecall/api",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
  "dev": "set PORT=4001 && tsx watch src/index.ts",
  "db:generate":"node --loader tsx ./node_modules/drizzle-kit/bin.cjs generate --config ./drizzle.config.ts",
  "db:migrate": "drizzle-kit migrate --config ./drizzle.config.ts",
  "build": "tsc",
  "start": "node dist/index.js",
  "lint": "eslint .",
  "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bridgecall/shared": "workspace:*",
    "dotenv": "16.4.5",
    "fastify": "4.28.1",
    "pg": "8.12.0"
  },
  "devDependencies": {
    "@types/node": "20.14.11",
    "@types/pg": "8.11.6",
    "tsx": "4.15.6",
    "typescript": "5.5.3"
  }
}
